{
  "version": 2,
  "functions": {
    "php/*.php": {
      "runtime": "vercel-php@0.5.0"
    }
  },
  "routes": [
    {
      "src": "/php/(.*)",
      "dest": "/php/$1"
    }
  ],
  "regions": ["syd1"],
  "crons": [
    {
      "path": "/api/notify-maintenance",
      "schedule": "*/2 * * * *"
    }
  ]
}
